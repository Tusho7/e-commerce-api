datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Users {
    id             Int     @id @default(autoincrement())
    email          String  @unique
    firstName      String?
    lastName       String?
    password       String
    profilePicture String?

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
